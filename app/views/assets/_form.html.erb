<%= form_for @asset do |f| %>
  <%= f.error_messages %>
  <p>
    <%= f.label :user_id %><br />
    <%= f.text_field :user_id %>
  </p>
  <p><%= f.submit %></p>
<% end %>


<%= form_for @asset, :html => {:multipart => true} do |f| %>
  <%= f.error_messages %>
  <p>
      <%= f.label :uploaded_file, "File" %><br>
      <%= f.file_field :uploaded_file %>
    </p>
  <p><%= f.submit "Upload" %></p>
<% end %>
